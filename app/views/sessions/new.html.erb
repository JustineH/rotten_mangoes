<div class="container">
  <h1>Rotten Mangoes</h1>
  <h2>Log In</h2>
   <hr>
  <%= form_tag sessions_path do %>
    <div>
      <%= label_tag :email %><br/>
      <%= text_field_tag :email %>
    </div>
    <br>
    <div>
      <%= label_tag :password %><br/>
      <%= password_field_tag :password %>
    </div>
    <br>
    <div><%= submit_tag "Log In", class: "btn btn-primary" %></div>
  <% end %>
</div>
